<?xml version='1.0' encoding='utf-8'?>
<widget id="com.clevertap.cordovastarter" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Starter</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.4.0" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="com.clevertap.cordova.CleverTapPlugin" spec="https://github.com/CleverTap/clevertap-cordova.git">
        <variable name="CLEVERTAP_ACCOUNT_ID" value="69R-Z7W-W54Z" />
        <variable name="CLEVERTAP_TOKEN" value="0a1-2c6" />
    </plugin>
</widget>
